<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mps.kodeaddict.eaa">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".eaa"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.CandidateListActivity"/>
        <activity android:name=".view.PartyListActivity"/>
        <activity android:name=".view.LoginActivity"/>
        <activity android:name=".view.SignupActivit"/>
        <activity android:name=".view.ChangePasswordActivity"/>
        <activity android:name=".view.ChoseLocationActivity"/>
        <activity android:name=".view.FaqListActivity"/>
    </application>

</manifest>
